local scripts = {
    'https://raw.githubusercontent.com/NotH4xor/Bahasf/refs/heads/main/Spawn%20Trap/Final%20Spawntrap',
    'https://raw.githubusercontent.com/NotH4xor/Bahasf/refs/heads/main/Baseplate%20Removal',
    'https://raw.githubusercontent.com/NotH4xor/Bahasf/refs/heads/main/Auto%20Rejoin/anti-kick',
    'https://raw.githubusercontent.com/NotH4xor/Bahasf/refs/heads/main/HKILL',
    'https://raw.githubusercontent.com/NotH4xor/Bahasf/refs/heads/main/TEST%20ZERO%20PPL%20SERVERHOP',
}

for _, url in ipairs(scripts) do
    local thread = coroutine.create(function()
        loadstring(game:HttpGet(url, true))()
    end)
    coroutine.resume(thread)
end

table.insert(scripts, "new_script_url")
